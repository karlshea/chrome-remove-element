{
  "name": "Remove Element",
  "version": "1.0.2",
  "description": "Creates a contextual menu item to remove an element from the DOM",
  "manifest_version": 2,
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "remove.js"
      ]
    }
  ]
}
