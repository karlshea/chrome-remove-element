{
  "name": "Remove Element",
  "version": "1.1.0",
  "description": "Creates a contextual menu item to remove an element from the DOM",
  "manifest_version": 3,
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": ["remove.js"],
      "all_frames": true,
      "run_at": "document_start"
    }
  ]
}
